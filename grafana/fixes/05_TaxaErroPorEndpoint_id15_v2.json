{
  "datasource": {
    "uid": "cex2jo3696xhca",
    "type": "elasticsearch"
  },
  "description": "Tabela consolidada por endpoint: Total de requisições, Total de erros e Percentual de erro.",
  "fieldConfig": {
    "defaults": { 
      "color": { "mode": "palette-classic" }, 
      "mappings": [], 
      "unit": "short",
      "decimals": 0
    },
    "overrides": [
      {
        "matcher": { "id": "byName", "options": "Endpoint" },
        "properties": [
          { "id": "custom.width", "value": 350 }
        ]
      },
      {
        "matcher": { "id": "byName", "options": "Total Requisições" },
        "properties": [
          { "id": "custom.width", "value": 130 },
          { "id": "color", "value": { "fixedColor": "rgb(45, 138, 216)", "mode": "fixed" }}
        ]
      },
      {
        "matcher": { "id": "byName", "options": "Total Erros" },
        "properties": [
          { "id": "custom.width", "value": 100 },
          { "id": "color", "value": { "fixedColor": "rgb(216, 123, 0)", "mode": "fixed" }}
        ]
      },
      {
        "matcher": { "id": "byName", "options": "Taxa de Erro (%)" },
        "properties": [
          { "id": "unit", "value": "percent" },
          { "id": "decimals", "value": 2 },
          { "id": "custom.width", "value": 130 },
          { "id": "thresholds", "value": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "rgb(216, 123, 0)", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }},
          { "id": "custom.cellOptions", "value": { "type": "color-background" }}
        ]
      }
    ]
  },
  "gridPos": { "h": 8, "w": 12, "x": 12, "y": 39 },
  "id": 15,
  "options": { 
    "showHeader": true,
    "sortBy": [{ "displayName": "Taxa de Erro (%)", "desc": true }],
    "footer": {
      "show": false
    }
  },
  "pluginVersion": "12.0.0",
  "targets": [
    {
      "alias": "Total Requisições",
      "bucketAggs": [
        { 
          "field": "f5.url.keyword", 
          "id": "2", 
          "settings": { 
            "size": "50", 
            "order": "desc", 
            "orderBy": "_count",
            "min_doc_count": "1"
          }, 
          "type": "terms" 
        }
      ],
      "datasource": {
        "uid": "cex2jo3696xhca",
        "type": "elasticsearch"
      },
      "metrics": [
        { 
          "id": "1", 
          "type": "count"
        }
      ],
      "query": "(f5.vhost.keyword:\"integracao.caixavidaeprevidencia.com.br\" OR f5.vhost.keyword:\"integracao.caixavidaeprevidencia.intranet\") AND f5.url.keyword:/\\/api\\/*/",
      "refId": "A",
      "timeField": "timestamp"
    },
    {
      "alias": "Total Erros",
      "bucketAggs": [
        { 
          "field": "f5.url.keyword", 
          "id": "2", 
          "settings": { 
            "size": "50", 
            "order": "desc", 
            "orderBy": "_count",
            "min_doc_count": "0"
          }, 
          "type": "terms" 
        }
      ],
      "datasource": {
        "uid": "cex2jo3696xhca",
        "type": "elasticsearch"
      },
      "metrics": [
        { 
          "id": "1", 
          "type": "count"
        }
      ],
      "query": "(f5.vhost.keyword:\"integracao.caixavidaeprevidencia.com.br\" OR f5.vhost.keyword:\"integracao.caixavidaeprevidencia.intranet\") AND f5.url.keyword:/\\/api\\/*/ AND f5.status:[400 TO 599]",
      "refId": "B",
      "timeField": "timestamp"
    }
  ],
  "transformations": [
    {
      "id": "seriesToColumns",
      "options": {
        "byField": "f5.url.keyword"
      }
    },
    {
      "id": "organize",
      "options": {
        "excludeByName": {},
        "indexByName": {},
        "renameByName": {
          "f5.url.keyword": "Endpoint",
          "Count 1": "Total Requisições",
          "Count 2": "Total Erros"
        }
      }
    },
    {
      "id": "calculateField",
      "options": {
        "mode": "binary",
        "binary": {
          "left": "Total Erros",
          "right": "Total Requisições",
          "operator": "/"
        },
        "alias": "Taxa de Erro (%)",
        "replaceFields": false,
        "reduce": {
          "reducer": "sum"
        }
      }
    },
    {
      "id": "calculateField",
      "options": {
        "mode": "binary",
        "binary": {
          "left": "Taxa de Erro (%)",
          "right": "100",
          "operator": "*"
        },
        "alias": "Taxa de Erro (%)",
        "replaceFields": true
      }
    },
    {
      "id": "filterFieldsByName",
      "options": {
        "include": {
          "names": ["Endpoint", "Total Requisições", "Total Erros", "Taxa de Erro (%)"]
        }
      }
    },
    {
      "id": "sortBy",
      "options": {
        "fields": {},
        "sort": [
          { "field": "Taxa de Erro (%)", "desc": true }
        ]
      }
    }
  ],
  "title": "Taxa de Erro por Endpoint",
  "transparent": true,
  "type": "table"
}
